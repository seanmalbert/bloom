{
  "name": "@bloom-housing/partners",
  "version": "1.0.3",
  "description": "Partners app reference implementation for the Bloom affordable housing system",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect=9230' next -p ${NEXTJS_PORT:-3001}",
    "build": "next build",
    "test": "concurrently \"yarn dev\" \"cypress open\"",
    "export": "next export",
    "start": "next start",
    "dev:listings": "cd ../../backend/core && yarn dev",
    "dev:server-wait": "wait-on \"http-get://localhost:${PORT:-3100}/listings\" && yarn dev",
    "dev:all": "concurrently \"yarn dev:listings\" \"yarn dev:server-wait\""
  },
  "dependencies": {
    "@bloom-housing/ui-components": "^1.0.3",
    "@zeit/next-sass": "^1.0.1",
    "ag-grid-community": "^24.1.0",
    "ag-grid-react": "^24.1.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "nanoid": "^3.1.12",
    "next": "^10.1.0",
    "next-plugin-custom-babel-config": "^1.0.2",
    "node-polyglot": "^2.4.0",
    "node-sass": "^4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.9.2",
    "swr": "^0.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@cypress/webpack-preprocessor": "^5.4.6",
    "@next/bundle-analyzer": "^10.1.0",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.52",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cypress": "^4.12.1",
    "js-levenshtein": "^1.1.6",
    "next-transpile-modules": "^4.1.0",
    "sass-loader": "^10.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2"
  }
}
