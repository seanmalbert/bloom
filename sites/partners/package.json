{
  "name": "@bloom-housing/partners",
  "version": "1.0.5",
  "description": "Partners app reference implementation for the Bloom affordable housing system",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect=9231' next -p ${NEXTJS_PORT:-3001}",
    "build": "next build",
    "test": "concurrently \"yarn dev\" \"cypress open\"",
    "test:headless": "concurrently \"yarn dev\" \"cypress run\"",
    "export": "next export",
    "start": "next start",
    "dev:listings": "cd ../../backend/core && yarn dev",
    "dev:server-wait": "wait-on \"http-get://localhost:${PORT:-3100}/listings\" && yarn dev",
    "dev:all": "concurrently \"yarn dev:listings\" \"yarn dev:server-wait\""
  },
  "dependencies": {
    "@bloom-housing/ui-components": "^1.0.5",
    "@mapbox/mapbox-sdk": "^0.13.0",
    "@zeit/next-sass": "^1.0.1",
    "ag-grid-community": "^24.1.0",
    "ag-grid-react": "^24.1.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "electron": "^13.1.7",
    "moment": "^2.29.1",
    "nanoid": "^3.1.12",
    "next": "^11.1.1",
    "next-plugin-custom-babel-config": "^1.0.2",
    "node-polyglot": "^2.4.0",
    "node-sass": "^4.14.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.5",
    "swr": "^0.3.5",
    "tailwindcss": "2.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@cypress/webpack-preprocessor": "^5.4.6",
    "@next/bundle-analyzer": "^10.1.0",
    "@types/mapbox__mapbox-sdk": "^0.13.2",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.52",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cypress": "^4.12.1",
    "js-levenshtein": "^1.1.6",
    "next-transpile-modules": "^8.0.0",
    "postcss": "^8.3.6",
    "sass-loader": "^10.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2"
  }
}
